{
  "build": {
    "eas-dev": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development",
        "EXPO_PUBLIC_STORYBOOK_ENABLED": "false"
      },
      "ios": {
        "simulator": true
      }
    },
    "eas-pre": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview",
        "EXPO_PUBLIC_STORYBOOK_ENABLED": "false"
      }
    },
    "eas-prod": {
      "env": {
        "APP_VARIANT": "production",
        "EXPO_PUBLIC_STORYBOOK_ENABLED": "false"
      }
    },
    "local-dev": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development",
        "EXPO_PUBLIC_STORYBOOK_ENABLED": "false",
        "USE_LOCAL_FILES": "true"
      },
      "ios": {
        "simulator": true
      }
    },
    "local-pre": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview",
        "EXPO_PUBLIC_STORYBOOK_ENABLED": "false",
        "USE_LOCAL_FILES": "true"
      }
    }
  },
  "cli": {
    "version": ">= 3.10.0"
  },
  "submit": {
    "production": {}
  }
}
