{
  "cli": {
    "version": ">= 3.10.0"
  },
  "build": {
    "dev": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_STORYBOOK_ENABLED": "false",
        "APP_VARIANT": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_STORYBOOK_ENABLED": "false",
        "APP_VARIANT": "preview"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_STORYBOOK_ENABLED": "false",
        "APP_VARIANT": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
